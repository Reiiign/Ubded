            <string>if(alarm[0] &lt;= 0)// &amp;&amp; objZombie.instance_count &lt; MaxEnemiesAtOnce)
{
    //Reset Timer
    alarm[0] = WaveIntervalTime;
    
    //Quick fix to make the game get harder
    if(WaveIntervalTime &gt; 16)
        WaveIntervalTime -= 0.25;
    
    //Pick random direction
    var r = irandom_range(0,3);
    
    //Get view borders
    var viewleft = view_xview[0];
    var viewtop = view_yview[0];
    var viewright = viewleft + view_wview[0];
    var viewbottom = viewtop + view_hview[0];
    var xx = 0;
    var yy = 0;
    
    var ztype = noone;
    var zrand = irandom_range(0,100);
    if(zrand &lt;= 15)
    {
        ztype = objZombieSuicider;
    }
    else if(zrand &gt; 15 &amp;&amp; zrand &lt; 35) 
    {
        ztype = objZombieSpitter;
    }
    else
    {
        ztype = objZombie;
    }
    
    switch(r)
    {
        case 0://Up
            yy = viewtop - 32;
            xx = irandom_range(viewleft, viewright);
            break;
        case 1://Right
            xx = viewright + 32;
            yy = irandom_range(viewtop, viewbottom);
            break;
        case 2://Down
            yy = viewbottom + 32;
            xx = irandom_range(viewleft, viewright);
            break;
        case 3://Left
            xx = viewleft - 32;
            yy = irandom_range(viewtop, viewbottom);
            break;
    }
    if(!place_meeting(xx,yy,objWall))
    {
        show_debug_message(string(xx)+" "+string(yy));
        instance_create(xx,yy,ztype);
    }
}
